# Staging Environment Configuration
NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://api-staging.autolodge.com
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_DOMAIN=auth-staging.autolodge.com
VITE_JWT_ISSUER=autolodge-staging

# External Services
VITE_GOOGLE_MAPS_API_KEY=
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# CDN Configuration
VITE_CDN_BASE_URL=https://cdn-staging.autolodge.com
VITE_ASSETS_BASE_URL=https://assets-staging.autolodge.com

# Application Configuration
VITE_APP_NAME=AutoLodge (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION="SADC Vehicle Rental Marketplace - Staging"

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_CACHE_MAX_AGE=86400

# Monitoring
VITE_LOG_LEVEL=warn
VITE_ENABLE_ERROR_REPORTING=true