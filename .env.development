# Development Environment Configuration
NODE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_DOMAIN=localhost
VITE_JWT_ISSUER=autolodge-development

# External Services (use test keys)
VITE_GOOGLE_MAPS_API_KEY=
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_
VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# CDN Configuration (local development)
VITE_CDN_BASE_URL=http://localhost:3001
VITE_ASSETS_BASE_URL=http://localhost:3001

# Application Configuration
VITE_APP_NAME=AutoLodge (Development)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION="SADC Vehicle Rental Marketplace - Development"

# Security (relaxed for development)
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS_ONLY=false

# Performance (disabled for development)
VITE_ENABLE_COMPRESSION=false
VITE_ENABLE_CACHING=false
VITE_CACHE_MAX_AGE=0

# Monitoring (verbose for development)
VITE_LOG_LEVEL=debug
VITE_ENABLE_ERROR_REPORTING=false